<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic
  PUBLIC "-//OASIS//DTD DITA Composite//EN" "ditabase.dtd">
<topic id="topic1" xml:lang="en">
  <title id="fc138336">gp_fastsequence</title>
  <body>
    <p>The <codeph>gp_fastsequence</codeph> table contains information about append-optimized and
      column-oriented tables. The <codeph>last_sequence</codeph> value indicates maximum row number
      currently used by the table.</p>
    <table id="fc138428">
      <title>pg_catalog.gp_fastsequence</title>
      <tgroup cols="4">
        <colspec colnum="1" colname="col1" colwidth="131pt"/>
        <colspec colnum="2" colname="col2" colwidth="97pt"/>
        <colspec colnum="3" colname="col3" colwidth="82pt"/>
        <colspec colnum="4" colname="col4" colwidth="138pt"/>
        <thead>
          <row>
            <entry colname="col1">column</entry>
            <entry colname="col2">type</entry>
            <entry colname="col3">references</entry>
            <entry colname="col4">description</entry>
          </row>
        </thead>
        <tbody>
          <row>
            <entry colname="col1">
              <codeph>objid</codeph>
            </entry>
            <entry colname="col2">oid</entry>
            <entry colname="col3">pg_class.oid</entry>
            <entry colname="col4">Object id of the <codeph>pg_aoseg.pg_aocsseg_*</codeph> table used
              to track append-optimized file segments.</entry>
          </row>
          <row>
            <entry colname="col1">
              <codeph>objmod</codeph>
            </entry>
            <entry colname="col2">bigint</entry>
            <entry colname="col3"/>
            <entry colname="col4">Object modifier.</entry>
          </row>
          <row>
            <entry colname="col1">
              <codeph id="fc138567">last_sequence</codeph>
            </entry>
            <entry colname="col2">bigint</entry>
            <entry colname="col3"/>
            <entry colname="col4">The last sequence number used by the object.</entry>
          </row>
        </tbody>
      </tgroup>
    </table>
  </body>
</topic>
